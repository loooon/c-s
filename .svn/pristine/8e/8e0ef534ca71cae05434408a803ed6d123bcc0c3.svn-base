body,html{margin:0 auto;min-height:100%; width:100%; color: #444;}
	body{font-family:"微软雅黑",'Microsoft YaHei',Arial,sans-serif; font-size: 14px;}
	h1,h2,h3,h4,h5,p,i,b,em{ font-style:normal; font-weight:normal;margin: 0;}
	ul,li{ list-style:none;}
	ul{ margin: 0 !important; padding: 0;}
	*,*:active,*:focus{ outline: 0 none;}
	img{border:none; vertical-align:middle;}
	input {outline: none; color:#444; margin:0; padding:0; font-family: "微软雅黑";}
	a{text-decoration: none;}
	a:active{outline:none;}
.layout{width: 1100px;margin:0 auto;}
.navbar{height: 80px;}
.navbar a.logo{display: inline-block;float: left;margin-top: 20px;}
.navbar ul{float: right;display: inline-block;right: 0;}	
.navbar ul li{display: inline-block;float: left;margin-left: 44px;line-height: 80px;}	
.navbar ul li a{color: #888;}	
.header{background: url(../../img/dun/header1.jpg)no-repeat center;width: 100%;height: 482px;}
.number{height: 52px;background-color: #011825;text-align: center;}	
.number p{line-height: 52px;color: #fff;font-size: 24px;}	
.number p span{color: #ff476d;font-size: 27px;}	
.icon{margin: 75px auto;overflow: hidden;}
.icon li{margin: 0 20px;padding-right: 40px;width: 200px;text-align: center;display: inline-block;float: left;
font-size: 18px;margin-top: 30px;}	
.icon li em{width: 95px;height: 95px;display: block;margin: 0 auto;}
.icon li em.em01{background: url(../../img/dun/icon1.png) no-repeat;}
.icon li em.em01:hover{background: url(../../img/dun/icon1.png) no-repeat 0 -105px;}
.icon li em.em02{background: url(../../img/dun/icon2.png) no-repeat;}
.icon li em.em02:hover{background: url(../../img/dun/icon2.png) no-repeat 0 -105px;}
.icon li em.em03{background: url(../../img/dun/icon3.png) no-repeat;}
.icon li em.em03:hover{background: url(../../img/dun/icon3.png) no-repeat 0 -105px;}
.icon li em.em04{background: url(../../img/dun/icon4.png) no-repeat;}
.icon li em.em04:hover{background: url(../../img/dun/icon4.png) no-repeat 0 -105px;}
.icon li p:hover{font-weight: bold;}
.icon a{color: #444;}
.icon a:hover{color: #444;}
.copy{background-color: #efefef;width: 100%;height: 42px;text-align: center;}	
.copy p{line-height: 42px;}		
.side{ position: fixed; bottom: 100px;right: 0;z-index: 1;}
.side-pic{right: 0;position: relative;}
.pic,.pic01{width: 39px;height: 39px;right: 0;float: right;z-index:100;}
.back,.back01{width: 152px;height: 39px;position: absolute;right: 0;float: right;}
.pic{background: url(../../img/dun/link01.png) 0 0 no-repeat;z-index:1;}
.back{background: url(../../img/dun/link02.png) 0 0 no-repeat;z-index:2;}
.side-pic01{position: relative;float: right;right: 0;}
.pic01{background: url(../../img/dun/link03.png) 0 0 no-repeat;margin-top: 8px;z-index:1;}
.back01{background: url(../../img/dun/link04.png) 0 0 no-repeat;width: 143px;height: 98px; z-index: 4; position: relative;margin-top: -51px;margin-right: -39px;}

.nub{background: url(../../img/dun/bg01.jpg) no-repeat center;width: 100%;height: 108px;text-align: right;}
.nub h4{font-size: 40px;color: #fff;padding: 15px 0 5px;margin-right: 2%;}
.nub h5{font-size: 16px;color: #fff;margin-right: 2%;}
.index{background: url(../../img/dun/index-bg.jpg) no-repeat center;width: 100%;height: 686px;}
.index-main{padding-top: 97px;}
.index-left{display: inline-block;float: left;padding: 0 70px 0 97px;}
.index-right{display: inline-block;float: left;}
.index-right h4{font-size: 30px;color: #fff;padding-top: 146px;padding-bottom: 35px;}
.index-in{background: url(../../img/dun/input.png) no-repeat center;width: 606px;height: 52px;}
.index-in input{display: inline-block;}
.index-in input.index-in1{width: 428px;height: 46px;margin-left: ;padding: 0 10px;line-height: 46px;font-size: 18px;
margin-left: 50px;float: left;margin-top: 3px;background-color: transparent;border: 0;color: #dcdcdc;}
.index-in input.index-in2{width: 90px;height: 50px;background-color: #118dd9;color: #fff;line-height: 50px;
font-size: 16px;float: right;border: 0;}

.header01{background: url(../../img/dun/header2.jpg)no-repeat center;width: 100%;height: 207px;}
.apply-content{background: url(../../img/dun/apply-bg.png)no-repeat bottom;min-height: 320px;}
.apply-input{width: 500px;margin: 75px auto 0;overflow: hidden;padding-bottom: 90px;}
.apply-block{padding-bottom: 60px;position: relative;overflow: hidden;}
.apply-block p{display: block;clear: both;}
.apply-block p span{display: inline-block;width: 135px;float: left;line-height: 43px;font-size: 20px;}
.apply-block p span i{color: #c11920;font-weight: bold;}
.apply-block p input{display: inline-block;width: 340px;float: left;border: 1px solid #a9a9a9;height: 41px;
line-height: 41px;padding: 0 10px; color: #c9c9c9;}
.apply-block p.apply-p{position: absolute;width: 100%;top: 47px;}
.apply-block p em{color: #c11920;font-size:15px;margin-left: 135px;}
.apply-block p em img{margin-right:5px;vertical-align: middle;}
.apply-bot{position: relative;}
.apply-sud{position: absolute;width: 100%;top: -33px;}
.apply-sud p.apply-p{top: 0;text-align: center;}
.apply-sud p.apply-p em{font-weight: bold;margin-left: 0;color: #c11920;}
.apply-btn{position: relative;right: 0;float: right;}
.apply-btn input{background-color: #118dd9;color: #fff;height: 41px;line-height: 41px;width: 150px;font-size: 16px;
border: none;color: #fff;}

.header02{background: url(../../img/dun/header3.jpg)no-repeat center;width: 100%;height: 207px;}

