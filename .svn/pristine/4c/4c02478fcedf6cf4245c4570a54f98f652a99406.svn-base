    <script type="text/javascript" src="/resources/js/dun/companyApply.js"></script>
</head>
<body>
	<nav class="navbar navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<a href="/dun/index" class="navbar-brand">
					<img class="logo-light" src="/resources/img/dun/mobile/logo.png" alt="">
				</a>
			</div>
		</div>
	</nav>
    <div class="adv adv-top">
    	<img src="/resources/img/dun/mobile/header-bg1.jpg">
    </div>
    <div class="apply-input">
        <form id="applyForm">
    	<div class="apply-block">
    		<p><span><i>*</i>公司名称：</span><input type="text" name="companyName" id="companyName" value=""></p>
    		<p class="apply-p"><em id="companyNameMessage" style="display:none;"><img src="/resources/img/dun/mobile/infor.png">请输入正确的公司名称</em></p>
    	</div>
    	<div class="apply-block">
    		<p><span><i>*</i>联系人：</span><input type="text" value="" name="contacts" id="contacts"></p>
    		<p class="apply-p"><em id="contactsMessage"  style="display:none;"><img src="/resources/img/dun/mobile/infor.png">请输入正确的联系人姓名</em></p>
    	</div>
    	<div class="apply-block">
    		<p><span><i>*</i>手机号码：</span><input type="text" value="" name="phone" id="phone"></p>
    		<p class="apply-p"><em id="phoneMessage"   style="display:none;"><img src="/resources/img/dun/mobile/infor.png">请输入正确的手机号码</em></p>
    	</div>
            <div class="apply-block apply-sud">
                <p class="apply-p"><em id="message" style="display: none">申请已成功</em></p>
            </div>
            <div class="apply-btn">
                <p><input type="button" id="submitApply" value="提交申请"></p>
    	</div>
    	<div class="apply-bg"><img src="/resources/img/dun/mobile/apply-bg.png"></div>
        </form>
    </div>
    <div id="pop" class="adv res" style="display: none">
        <img src="/resources/img/dun/mobile/pop.png">
    </div>
    <div class="apply-bot">
    <div class="adv number">
    	<img src="/resources/img/dun/mobile/adv09.jpg">
    	<div class="content crp-showcase">
		<div class="col-sm-12 adm">
            <h1 class="sourehan text-center animated fadeInDown">凭安已发现<span id="dunCount">1601711</span>个催收号码</h1>
		</div>
		</div>
    </div>
    <div class="adv adv-bot">
    	<img src="/resources/img/dun/mobile/adv08.jpg">
    </div>
	</div>
