body,html{margin:0 auto;min-height:100%; width:100%; color: #444;}
	body{font-family:"微软雅黑",'Microsoft YaHei',Arial,sans-serif; font-size: 14px;}
	h1,h2,h3,h4,h5,p,i,b,em{ font-style:normal; font-weight:normal;margin: 0;}
	ul,li{ list-style:none;}
	ul{ margin: 0 !important; padding: 0;}
	*,*:active,*:focus{ outline: 0 none;}
	img{border:none; vertical-align:middle;}
	input {outline: none; color:#444; margin:0; padding:0; font-family: "微软雅黑";}
	a{text-decoration: none;}
	a:active{outline:none;}
.layout{width: 1100px;margin:0 auto;}
.navbar{height: 80px;}
.navbar a.logo{display: inline-block;float: left;margin-top: 23px;}
.navbar ul{float: right;display: inline-block;right: 0;}	
.navbar ul li{display: inline-block;float: left;margin-left: 44px;line-height: 80px;}	
.navbar ul li a{color: #888;}
.navbar ul li a:hover,.navbar ul li a.current{color: #4a4a4a;}
.header{background: url(../../img/dun/header1.jpg)no-repeat center;width: 100%;height: 335px;}
.number{height: 52px;background-color: #011825;text-align: center;}	
.number p{line-height: 52px;color: #fff;font-size: 24px;}	
.number p span{color: #ff476d;font-size: 27px;padding: 0 5px;}	
.icon{margin: 75px auto;overflow: hidden;}
.icon li{margin: 0 20px;padding-right: 40px;width: 200px;text-align: center;display: inline-block;float: left;
font-size: 18px;margin-top: 30px;}	
.icon li em{width: 95px;height: 95px;display: block;margin: 0 auto;}
.icon li em.em01{background: url(../../img/dun/icon1.png) no-repeat;}
.icon li em.em01:hover{background: url(../../img/dun/icon1.png) no-repeat 0 -105px;}
.icon li em.em02{background: url(../../img/dun/icon2.png) no-repeat;}
.icon li em.em02:hover{background: url(../../img/dun/icon2.png) no-repeat 0 -105px;}
.icon li em.em03{background: url(../../img/dun/icon3.png) no-repeat;}
.icon li em.em03:hover{background: url(../../img/dun/icon3.png) no-repeat 0 -105px;}
.icon li em.em04{background: url(../../img/dun/icon4.png) no-repeat;}
.icon li em.em04:hover{background: url(../../img/dun/icon4.png) no-repeat 0 -105px;}
.icon li p:hover{font-weight: bold;}
.icon a{color: #444;}
.icon a:hover{color: #444;}
.footer{background-color: #f1f3f6;width: 100%;height: 150px;}
.f-logo{padding-top: 36px;width: 285px;display: inline-block;float: left;}
.f-main{padding-top: 44px;width: 707px;display: inline-block;float: left;}
.f-main span{margin-bottom: 25px;display: inline-block;}
.f-main em{background: url(../../img/dun/icon10.png) no-repeat;width: 22px;height: 25px;display: inline-block;
vertical-align: middle;padding-right: 5px;}
.f-main em.em1{background-position: 0 0;}
.f-main em.em2{background-position: 0 -61px;}
.f-main em.em3{background-position: 0 -31px;}
.f-main em.em4{background-position: 0 -94px;}
.f-w{width: 215px;}
.f-ewm{display: inline-block;float: right;padding-top: 22px;width: 108px;}
.copy{background-color: #efefef;width: 100%;height: 42px;text-align: center;}	
.copy p{line-height: 42px;}		
.header03{background: url(../../img/dun/header4.jpg)no-repeat center;width: 100%;height: 335px;}
.index-main{padding: 80px 0 145px;}
.index-in{background: url(../../img/dun/input.png) no-repeat center;width: 606px;height: 52px;margin: 0 auto;position: relative;}
.index-in input{display: inline-block;}
.index-in input.index-in1{width: 428px;height: 46px;padding: 0 10px;line-height: 46px;font-size: 18px;
margin-left: 50px;float: left;margin-top: 3px;background-color: transparent;border: 0;color: #444;}
.index-in input.index-in2{width: 90px;height: 50px;background-color: #118dd9;color: #fff;line-height: 50px;
font-size: 16px;float: right;border: 0;}
.index-main p.apply-p{position: absolute;top: 65px;left: 18px;}
.index-main p.apply-p em{color: #c11920;font-size: 15px;vertical-align: middle;}
.index-main p.apply-p img{margin-right: 5px;}
.header01{background: url(../../img/dun/header2.jpg)no-repeat center;width: 100%;height: 335px;}
.apply-content{background: url(../../img/dun/apply-bg.png)no-repeat bottom;min-height: 310px;}
.apply-input{width: 500px;margin: 75px auto 0;overflow: hidden;padding-bottom: 90px;}
.apply-block{padding-bottom: 60px;position: relative;overflow: hidden;}
.apply-block p{display: block;clear: both;}
.apply-block p span{display: inline-block;width: 135px;float: left;line-height: 43px;font-size: 20px;}
.apply-block p span i{color: #c11920;font-weight: bold;}
.apply-block p input{display: inline-block;width: 340px;float: left;border: 1px solid #a9a9a9;height: 41px;
line-height: 41px;padding: 0 10px; color: #444;font-size: 16px;}
.apply-block p.apply-p{position: absolute;width: 100%;top: 47px;}
.apply-block p em{color: #c11920;font-size:15px;margin-left: 135px;vertical-align: middle;}
.apply-block p em img{margin-right:5px;}
.apply-bot{position: relative;}
.apply-sud{position: absolute;width: 100%;top: -33px;}
.apply-sud p.apply-p{top: 0;text-align: center;}
.apply-sud p.apply-p em{font-weight: bold;margin-left: 0;color: #c11920;}
.apply-btn{position: relative;right: 0;text-align: center}
.apply-btn input{background-color: #118dd9;color: #fff;height: 41px;line-height: 41px;width: 150px;font-size: 16px;
border: none;color: #fff;}
.header02{background: url(../../img/dun/header3.jpg)no-repeat center;width: 100%;height: 207px;}
.file{margin: 70px auto 50px;width: 625px;min-height: 300px;}
.file span{display: inline-block;}
.file span.file-font{width: 190px;float: left;font-size: 20px;line-height: 45px;}
.file span.file-font i{color: #c11920;padding-right: 5px;font-size: 20px;}
.file span.file-input{background: url(../../img/dun/input1.png)no-repeat center;width: 433px;height: 45px;}
.file-input input.file-in1{width: 292px;height: 38px;margin-left: 55px;margin-top: 2px;border: 0;line-height: 38px;
font-size: 16px;color: #444;}
.file-input input.file-in2{width: 80px;float: right;height: 45px;background-color: transparent;border: 0;}
.file p{margin-left: 190px;position: relative;}
.file p em{color: #c11920;font-size: 15px;position: absolute;top: 5px;}
.file p em img{vertical-align: middle;margin-right: 5px;}
.header04{background: url(../../img/dun/header5.jpg)no-repeat center;width: 100%;height: 335px;}
.from{width: 604px;margin: 77px auto 0;}
.from-block{margin-bottom: 75px;position: relative;width: 602px;height: 50px;border: 1px solid #ddd;
line-height: 50px;}
.from-block i{margin:0 18px;width: 21px;display: inline-block;float: left;}
.from-block p input{width: 540px;height: 48px;color: #d8d8d8;line-height: 48px;color: #444;font-size: 18px;
border: transparent;}
.from-block p.b1{position: absolute;color: #c11920;top: 48px;font-size: 15px;}
.from-block p.b1 em,.from-block p.b2 em{vertical-align: middle;padding-left: 5px;}
.from-block p.b2{position: absolute;color: #c11920;margin-top: 47px;}
.from-file{width: 540px;height: 48px;border: 1px solid #ddd;background-color: transparent;position: relative;
color: #444;border-radius: 4px;display: inline-block;float: left;z-index: 100;text-align: center;border: 0;}
.from-file input{opacity: 0;filter: alpha(opacity=0); z-index: 999;height: 30px;width: 100%;position: relative;
border-radius: 4px;}
.from-file b{position: absolute;top: 1px;left: 10px;z-index: 0;font-size: 18px;text-overflow: ellipsis;    
overflow: hidden; white-space: nowrap;width: 520px;text-align: left;}
.from-block-btn input{width: 150px;height:41px;line-height: 41px;text-align: center;border-radius: 2px;
background-color: #118dd9;color: #fff;cursor: pointer;margin-left: 200px;border: 0;font-size: 16px;}
.container{width: 100%;background-color: #f8f9fa;margin-top: 60px;}
.exp{padding: 75px 0;text-align: center;}
.exp p{font-size: 18px;line-height: 40px;text-indent: 2em;margin: 40px 0 45px;color: #4a4a4a;text-align: left;}
.exp p input{width: 220px;font-size: 18px;border: 0;border-bottom: 1px solid #4a4a4a;background-color: transparent;}
.search-file{margin-left: 56px;height: 40px;line-height: 40px;margin-top: 1px;background-color: #fff;border: 0;}
.search-file b{top: 1px;}

.pop{width: 556px;height: 364px;left:50%;top: 50%;margin-left: -278px;margin-top: -182px;background-color: #fff;
border-radius: 6px;position: absolute;}
.pop h2{height: 54px;line-height: 54px;border-bottom: 1px solid #ddd;margin: 0 28px;padding-top: 5px;}
.pop h2 span{font-size: 18px;}
.pop h2 a{float: right;display: inline-block;}
.pop-border{border: 0;background-color: #fff;display: inline-block;float: left;margin: 65px 0 70px;min-width: 407px;}
.res-left{width: 68px;float: left;display: inline-block;padding-right: 22px;padding-left: 110px;}
.res-right{float: left;display: inline-block;}
.res-right h4{color: #4a4a4a;font-size: 28px;padding-bottom: 5px;}
.res-right h5{color: #4a4a4a;font-size:20px;}
.pop input{width: 150px;height: 41px;line-height: 41px;text-align: center;color: #4a4a4a;background-color: #eee;
border: 0;margin:0  auto;display: block;font-size: 16px;}
.res-right h3{color: #4a4a4a;font-size: 24px;}
.adv{clear: both;margin: 0 auto 40px;width: 1080px;display: block;}
.adv img{margin-top: 30px;}

.result{background-color: #f7f7f7;min-height: 300px;padding: 32px 0 20px;margin-top: 80px;overflow: hidden}
.result-block01{border: 1px solid #ddd;background-color: #fff;width: 327px;height: 117px;margin: 0 19px 20px 18px;
display: inline-block;float: left;}
.res-left01{padding: 24px 22px 24px 22px;width: 63px;float: left;display: inline-block;}
.res-right01{width: 220px;float: left;display: inline-block;padding-top: 22px;}
.res-right01 h4{color: #4a4a4a;font-size: 22px;line-height: 31px;padding: 12px 0 2px;}
.res-right01 h5{color: #4a4a4a;font-size:15px;}
