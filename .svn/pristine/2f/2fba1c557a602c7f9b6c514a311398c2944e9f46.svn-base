.loader-container {
    height: 3px;
    /*overflow: hidden;*/
    width: 99.2%;
    position: absolute;
    top: 50%;
    /*left:20%;*/
    /*left: 50%;*/
    margin-top: -3px;
    /*margin-left: -300px;*/
    background-color: transparent;
    background-image: -webkit-linear-gradient(left, #ec493e, #fc852b,#d7e448,#7dd769,#2bb8a0);
    background-image: -moz-linear-gradient(left, #ec493e, #fc852b,#d7e448,#7dd769,#2bb8a0);
    background-image: -o-linear-gradient(left, #ec493e, #fc852b,#d7e448,#7dd769,#2bb8a0);
    background-image: -ms-linear-gradient(left, #ec493e, #fc852b,#d7e448,#7dd769,#2bb8a0);
    background-image: linear-gradient(left, #ec493e, #fc852b,#d7e448,#7dd769,#2bb8a0);
    box-shadow: inset 0 -2px 2px rgba(0, 0, 0, 0.4);
    /*border-radius: 3px 0 0 3px;*/
}
.loader-container:after {
    content: "";
    display: block;
    position: absolute;
    right: 0;
    top: 50%;
    width: 0.5em;
    height: 0.5em;
    border-radius: 50%;
    margin-top: -0.25em;
    margin-right: -0.25em;
    background-image: -webkit-linear-gradient(top, #000000, #212121);
    background-image: -moz-linear-gradient(top, #000000, #212121);
    background-image: -o-linear-gradient(top, #000000, #212121);
    background-image: -ms-linear-gradient(top, #000000, #212121);
    background-image: linear-gradient(top, #000000, #212121);
    animation: myfirst 10s linear;

}
.loader-container.done:after {
    background: #2bb8a0;
}
.run .runner {
    content: "";
    position: absolute;
    right: -3px;
    height: 100%;
    width: 0%;
    background-color: transparent;
    background-image: -webkit-linear-gradient(top, #f0f0f0, #f0f0f0);
    background-image: -moz-linear-gradient(top, #000000, #212121);
    background-image: -o-linear-gradient(top, #000000, #212121);
    background-image: -ms-linear-gradient(top, #000000, #212121);
    background-image: linear-gradient(top, #000000, #212121);
    animation: loader 10s linear;
}
.meter {
    position: absolute;
    top: 0;
    right: 0;
    font-size: 2em;
    margin-top: .3em;
    /*color: #ff0000;*/
    text-shadow: 0 -1px 0 #333333;
    animation: mete 10s linear;
}
.meter:after {
    content: "%";
}

@keyframes loader {
    0% {
        width: 100%;
    }
    100% {
        width: 0%;
    }
}
@-webkit-keyframes loader {
    0% {
        width: 100%;
    }
    100% {
        width: 0%;
    }
}
@-moz-keyframes loader {
    0% {
        width: 100%;
    }
    100% {
        width: 0%;
    }
}
@keyframes mete {
    0%   {color:#ec493e}
    25%  {color:#fc852b }
    50%  {color:#d7e448 }
    75%  {color:#7dd769 }
    100% {color:#2bb8a0}
}
@-webkit-keyframes mete {
    0%   {color:#ec493e}
    25%  {color:#fc852b }
    50%  {color:#d7e448 }
    75%  {color:#7dd769 }
    100% {color:#2bb8a0}
}
@-moz-keyframes mete {
    0%   {color:#ec493e}
    25%  {color:#fc852b }
    50%  {color:#d7e448 }
    75%  {color:#7dd769 }
    100% {color:#2bb8a0}
}
/* Decoration */



@keyframes myfirst
{
    0%   {left:0;background:#ec493e}
    25%  {left:25%;background:#fc852b }
    50%  {left:50%;background:#d7e448 }
    75%  {left:75%;background:#7dd769 }
    100% {left:100%; background:#2bb8a0}
}
@-webkit-keyframes myfirst
{
    0%   {left:0;background:#ec493e}
    25%  {left:25%;background:#fc852b }
    50%  {left:50%;background:#d7e448 }
    75%  {left:75%;background:#7dd769 }
    100% {left:100%; background:#2bb8a0}
}
@-moz-keyframes myfirst
{
    0%   {left:0;background:#ec493e}
    25%  {left:25%;background:#fc852b }
    50%  {left:50%;background:#d7e448 }
    75%  {left:75%;background:#7dd769 }
    100% {left:100%; background:#2bb8a0}
}
.stop {
    animation-play-state: paused;
}

#grad1 {
    position: absolute;
    /*margin-top: 300px;*/
    height: 14px;
    /*margin-left: 34%;*/
    /*margin-right: 34%;*/
    margin-top: 35px;
    width: 100%;
    background: -webkit-linear-gradient(left,#ec493e ,#fc852b,#d7e448, #7dd769,#2bb8a0); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right,#ec493e ,#fc852b,#d7e448, #7dd769,#2bb8a0); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #ec493e ,#fc852b,#d7e448, #7dd769,#2bb8a0); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #ec493e ,#fc852b,#d7e448, #7dd769,#2bb8a0); /* 标准的语法（必须放在最后） */
}